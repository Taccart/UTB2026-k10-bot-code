    "/udp/v1": {
      "get": {
        "tags": ["UDP"],
        "summary": "Get UDP service status",
        "description": "Returns UDP listening port, message statistics, and recent messages",
        "operationId": "getUDPStatus",
        "responses": {
          "200": {
            "description": "UDP status retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "port": {
                      "type": "integer",
                      "description": "UDP listening port"
                    },
                    "total": {
                      "type": "integer",
                      "description": "Total messages received since start"
                    },
                    "dropped": {
                      "type": "integer",
                      "description": "Number of packets dropped due to buffer lock"
                    },
                    "buffer": {
                      "type": "string",
                      "description": "Current buffer usage (used/max)"
                    },
                    "messages": {
                      "type": "array",
                      "description": "Recent messages with timestamps",
                      "items": {
                        "type": "string"
                      }
                    },
                    "error": {
                      "type": "string",
                      "description": "Error message if buffer is locked"
                    }
                  }
                },
                "example": {
                  "port": 12345,
                  "total": 1523,
                  "dropped": 5,
                  "buffer": "15/20",
                  "messages": [
                    "[125 ms] Hello",
                    "[230 ms] World"
                  ]
                }
              }
            }
          },
          "503": {
            "description": "Service not started"
          }
        }
      }
    },
    "/api/openapi.json": {
      "get": {
        "tags": ["OpenAPI"],
        "summary": "Get OpenAPI specification",
        "description": "Get OpenAPI 3.0.0 specification for all registered services including paths, parameters, request bodies, and response schemas",
        "operationId": "getOpenAPISpec",
        "responses": {
          "200": {
            "description": "OpenAPI specification retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "openapi": {
                      "type": "string"
                    },
                    "info": {
                      "type": "object"
                    },
                    "paths": {
                      "type": "object"
                    }
                  }
                },
                "example": {
                  "openapi": "3.0.0",
                  "info": {
                    "title": "K10 Bot API",
                    "version": "1.0.0"
                  },
                  "paths": {}
                }
              }
            }
          },
          "503": {
            "description": "Service not started"
          }
        }
      }
    },
    "/webcam/v1/snapshot": {
      "get": {
        "tags": ["Camera"],
        "summary": "Capture snapshot",
        "description": "Captures and returns a JPEG image from the camera",
        "operationId": "captureSnapshot",
        "responses": {
          "200": {
            "description": "Snapshot captured successfully",
            "content": {
              "image/jpeg": {
                "schema": {
                  "type": "string",
                  "format": "binary",
                  "description": "JPEG image data"
                }
              }
            }
          },
          "503": {
            "description": "Camera not initialized or service not started"
          }
        }
      }
    },
    "/webcam/v1/settings": {
      "put": {
        "tags": ["Camera"],
        "summary": "Update camera settings",
        "description": "Updates camera settings such as quality, framesize, brightness, contrast, and saturation",
        "operationId": "updateCameraSettings",
        "requestBody": {
          "description": "Camera settings to update",
          "required": false,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "quality": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 63,
                    "description": "JPEG quality (0-63, lower is better)"
                  },
                  "framesize": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 13,
                    "description": "Frame size (0-13)"
                  },
                  "brightness": {
                    "type": "integer",
                    "minimum": -2,
                    "maximum": 2,
                    "description": "Brightness (-2 to 2)"
                  },
                  "contrast": {
                    "type": "integer",
                    "minimum": -2,
                    "maximum": 2,
                    "description": "Contrast (-2 to 2)"
                  },
                  "saturation": {
                    "type": "integer",
                    "minimum": -2,
                    "maximum": 2,
                    "description": "Saturation (-2 to 2)"
                  }
                },
                "additionalProperties": false
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Settings applied successfully"
          },
          "400": {
            "description": "Invalid JSON body"
          },
          "422": {
            "description": "Missing required parameters"
          },
          "503": {
            "description": "Camera not initialized or service not started"
          }
        }
      }
    },
    "/webcam/v1/stream": {
      "get": {
        "tags": ["Camera"],
        "summary": "Start video stream",
        "description": "Returns a continuous MJPEG video stream",
        "operationId": "startVideoStream",
        "responses": {
          "200": {
            "description": "Stream started successfully",
            "content": {
              "multipart/x-mixed-replace": {
                "schema": {
                  "type": "string",
                  "format": "binary",
                  "description": "Continuous MJPEG video stream"
                }
              }
            }
          },
          "503": {
            "description": "Camera not initialized or service not started"
          }
        }
      }
    },
    "/webcam/v1/stop": {
      "post": {
        "tags": ["Camera"],
        "summary": "Stop camera service",
        "description": "Stop the camera service (useful before changing framesize)",
        "operationId": "stopCameraService",
        "responses": {
          "200": {
            "description": "Service stopped successfully"
          },
          "500": {
            "description": "Failed to stop service"
          }
        }
      }
    },
    "/webcam/v1/start": {
      "post": {
        "tags": ["Camera"],
        "summary": "Start camera service",
        "description": "Start the camera service (applies saved framesize from preferences)",
        "operationId": "startCameraService",
        "responses": {
          "200": {
            "description": "Service started successfully"
          },
          "500": {
            "description": "Failed to start service"
          }
        }
      }
    },
    "/servos/v1/setServoAngle": {
      "post": {
        "tags": ["Servos"],
        "summary": "Set servo angle",
        "description": "Set servo angle for angular servos (180째 or 270째)",
        "operationId": "setServoAngle",
        "parameters": [
          {
            "name": "channel",
            "in": "query",
            "description": "Servo channel (0-7)",
            "required": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "angle",
            "in": "query",
            "description": "Angle in degrees (0-180 for 180째 servos, 0-270 for 270째 servos)",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Operation completed successfully"
          },
          "422": {
            "description": "Missing required parameters"
          },
          "500": {
            "description": "Operation failed"
          },
          "503": {
            "description": "Service not initialized or not started"
          }
        }
      }
    },
    "/servos/v1/setServoSpeed": {
      "post": {
        "tags": ["Servos"],
        "summary": "Set servo speed",
        "description": "Set continuous servo speed for rotational servos",
        "operationId": "setServoSpeed",
        "parameters": [
          {
            "name": "channel",
            "in": "query",
            "description": "Servo channel (0-7)",
            "required": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "speed",
            "in": "query",
            "description": "Speed percentage (-100 to +100, negative is reverse)",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Operation completed successfully"
          },
          "422": {
            "description": "Missing required parameters"
          },
          "500": {
            "description": "Operation failed"
          },
          "503": {
            "description": "Service not initialized or not started"
          }
        }
      }
    },
    "/servos/v1/stopAll": {
      "post": {
        "tags": ["Servos"],
        "summary": "Stop all servos",
        "description": "Stop all servos by setting speed to 0",
        "operationId": "stopAllServos",
        "responses": {
          "200": {
            "description": "Operation completed successfully"
          },
          "422": {
            "description": "Missing required parameters"
          },
          "500": {
            "description": "Operation failed"
          },
          "503": {
            "description": "Service not initialized or not started"
          }
        }
      }
    },
    "/music/v1/play": {
      "post": {
        "tags": ["Music"],
        "summary": "Play melody",
        "description": "Play built-in melody with playback options. Query parameters: melody (required, 0-19), option (optional, 1=Once, 2=Forever, 4=OnceInBackground, 8=ForeverInBackground)",
        "operationId": "playMelody",
        "parameters": [
          {
            "name": "melody",
            "in": "query",
            "description": "Melody enum value (0-19)",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
