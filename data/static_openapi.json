{
  "openapi": "3.0.0",
  "info": {
    "title": "K10 Bot API",
    "version": "1.0.0",
    "description": "API for controlling UniHiker K10 Robot with sensors, servos, camera, and more"
  },
  "servers": [
    {
      "url": "/api",
      "description": "API base path"
    }
  ],
  "tags": [
    {
      "name": "Board Info",
      "description": "System information and board metrics"
    },
    {
      "name": "Sensors",
      "description": "K10 sensor readings (light, temperature, humidity, accelerometer)"
    },
    {
      "name": "Logs",
      "description": "Rolling logger service for debug and application logs"
    },
    {
      "name": "UDP",
      "description": "UDP message handling and statistics"
    },
    {
      "name": "OpenAPI",
      "description": "API documentation and specification"
    },
    {
      "name": "Camera",
      "description": "Webcam snapshot and stream endpoints"
    },
    {
      "name": "Servos",
      "description": "Servo motor control (angle and speed)"
    },
    {
      "name": "Music",
      "description": "Music playback and tone generation"
    },
    {
      "name": "DFR1216",
      "description": "DFR1216 expansion board control"
    },
    {
      "name": "Settings",
      "description": "Persistent settings management"
    }
  ],
  "x-service-settings-pattern": "Each service that implements settings persistence automatically gets two routes: /{service}/saveSettings (GET) to save current settings, and /{service}/loadSettings (GET) to load persisted settings. These routes are not individually documented below but follow a standard pattern.",
  "x-service-status-pattern": "Some services expose a /serviceStatus (GET) route that returns the current service state (started, stopped, initialized, etc.).",
  "paths": {
    "/boardinfo/v1": {
      "get": {
        "tags": ["Board Info"],
        "summary": "Get board information",
        "description": "Retrieves comprehensive board information including system metrics, memory usage, chip details, and firmware version",
        "operationId": "getBoardInfo",
        "responses": {
          "200": {
            "description": "Board information retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "uptimeMs": {
                      "type": "integer",
                      "description": "System uptime in milliseconds"
                    },
                    "board": {
                      "type": "string",
                      "description": "Board model name"
                    },
                    "version": {
                      "type": "string",
                      "description": "Firmware version"
                    },
                    "heapTotal": {
                      "type": "integer",
                      "description": "Total heap size in bytes"
                    },
                    "heapFree": {
                      "type": "integer",
                      "description": "Free heap size in bytes"
                    },
                    "freeStackBytes": {
                      "type": "integer",
                      "description": "Free stack space for current task"
                    },
                    "chipCores": {
                      "type": "integer",
                      "description": "Number of CPU cores"
                    },
                    "chipModel": {
                      "type": "string",
                      "description": "Chip model name"
                    },
                    "chipRevision": {
                      "type": "integer",
                      "description": "Chip revision number"
                    },
                    "cpuFreqMHz": {
                      "type": "integer",
                      "description": "CPU frequency in MHz"
                    },
                    "freeSketchSpace": {
                      "type": "integer",
                      "description": "Available flash space for sketch"
                    },
                    "sdkVersion": {
                      "type": "string",
                      "description": "ESP-IDF SDK version"
                    }
                  }
                },
                "example": {
                  "uptimeMs": 123456,
                  "board": "UNIHIKER_K10",
                  "version": "1.0.0",
                  "heapTotal": 327680,
                  "heapFree": 280000,
                  "freeStackBytes": 2048,
                  "chipCores": 2,
                  "chipModel": "ESP32-S3",
                  "chipRevision": 1,
                  "cpuFreqMHz": 240,
                  "freeSketchSpace": 1310720,
                  "sdkVersion": "v4.4.2"
                }
              }
            }
          },
          "503": {
            "description": "Service not started",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "type": "string"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/sensors/v1": {
      "get": {
        "tags": ["Sensors"],
        "summary": "Get all sensor readings",
        "description": "Retrieves all K10 sensor readings including light, temperature, humidity, microphone, and accelerometer data",
        "operationId": "getSensors",
        "responses": {
          "200": {
            "description": "Sensor data retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "light": {
                      "type": "number",
                      "description": "Ambient light sensor reading"
                    },
                    "hum_rel": {
                      "type": "number",
                      "description": "Relative humidity percentage"
                    },
                    "celcius": {
                      "type": "number",
                      "description": "Temperature in Celsius"
                    },
                    "mic_data": {
                      "type": "number",
                      "description": "Microphone data reading"
                    },
                    "accelerometer": {
                      "type": "array",
                      "description": "3-axis accelerometer data [x,y,z]",
                      "items": {
                        "type": "number"
                      }
                    }
                  }
                },
                "example": {
                  "light": 125.5,
                  "hum_rel": 45.2,
                  "celcius": 23.8,
                  "mic_data": 512,
                  "accelerometer": [0.12, -0.05, 9.81]
                }
              }
            }
          },
          "503": {
            "description": "Sensor initialization or reading failed",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "type": "string"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/logs/v1/all": {
      "get": {
        "tags": ["Logs"],
        "summary": "Get all logs",
        "description": "Retrieves all log entries from both debug and app_info loggers",
        "operationId": "getAllLogs",
        "responses": {
          "200": {
            "description": "Log entries retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "debug": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "level": {
                            "type": "string"
                          },
                          "message": {
                            "type": "string"
                          }
                        }
                      }
                    },
                    "app_info": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "level": {
                            "type": "string"
                          },
                          "message": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                },
                "example": {
                  "debug": [
                    {
                      "level": "DEBUG",
                      "message": "WebServer task running..."
                    }
                  ],
                  "app_info": [
                    {
                      "level": "INFO",
                      "message": "WiFi connected"
                    }
                  ]
                }
              }
            }
          },
          "503": {
            "description": "Service not started"
          }
        }
      }
    },
    "/logs/v1/debug": {
      "get": {
        "tags": ["Logs"],
        "summary": "Get debug logs",
        "description": "Retrieves log entries from the debug logger only",
        "operationId": "getDebugLogs",
        "responses": {
          "200": {
            "description": "Log entries retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "level": {
                        "type": "string",
                        "description": "Log level"
                      },
                      "message": {
                        "type": "string",
                        "description": "Log message content"
                      }
                    }
                  }
                },
                "example": [
                  {
                    "level": "INFO",
                    "message": "System initialized"
                  },
                  {
                    "level": "DEBUG",
                    "message": "Service started"
                  }
                ]
              }
            }
          },
          "404": {
            "description": "Logger instance not available"
          }
        }
      }
    },
    "/logs/v1/app_info": {
      "get": {
        "tags": ["Logs"],
        "summary": "Get app info logs",
        "description": "Retrieves log entries from the app_info logger only",
        "operationId": "getAppInfoLogs",
        "responses": {
          "200": {
            "description": "Log entries retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "level": {
                        "type": "string",
                        "description": "Log level"
                      },
                      "message": {
                        "type": "string",
                        "description": "Log message content"
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Logger instance not available"
          },
          "503": {
            "description": "Service not started"
          }
        }
      }
    },
    "/logs/v1/esp": {
      "get": {
        "tags": ["Logs"],
        "summary": "Get ESP-IDF logs",
        "description": "Retrieves log entries from the ESP-IDF logger only",
        "operationId": "getESPLogs",
        "responses": {
          "200": {
            "description": "Log entries retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "level": {
                        "type": "string"
                      },
                      "message": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Logger instance not available"
          },
          "503": {
            "description": "Service not started"
          }
        }
      }
    },
    "/logs/v1/debug.json": {
      "get": {
        "tags": ["Logs"],
        "summary": "Get debug logs (JSON format)",
        "description": "Retrieves debug logger entries in JSON format",
        "operationId": "getDebugLogsJson",
        "responses": {
          "200": {
            "description": "Debug logs retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
